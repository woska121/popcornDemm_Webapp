<% include ../header.ejs %>
    <% include ./sidebar.ejs %>
     <div class="col-sm-10">
        <table class="table table-bordered table-hover">
            <h3><p class="text-center">회원 조회</p></h3>
            <br>
            <tr>
                <th>아이디</th>
                <th>닉네임</th>
                <th>회원가입일</th>
                <th>이메일인증여부</th>
                <th>활성상태</th>
                <th>사이버머니</th>
                <th>결제상품</th>
                <th>결제일</th>
                <th>남은일수</th>
            </tr>            
                <% user.forEach(function(user) { %>                 
            <tr>           
                <td><%=user.userid%></td>
                <td><%=user.displayname%></td>
                <td> 
                    <%=user.getDate.year%> -
                    <%=user.getDate.month%> - 
                    <%=user.getDate.day%>
                </td>
                <% if(user.subscription_status == true) {%>
                    <td>인증</td>
                <%} else {%>
                    <td>미인증</td>
                <%}%>

                <% if(user.state == true) {%>
                    <td>활성</td>
                <%} else {%>
                    <td>탈퇴</td>
                <%}%>
                <td></td>                        
                <td></td>
                <td></td>
                <td></td>
            </tr>            
            <%})%>
        </table>

<% include ../footer.ejs %>