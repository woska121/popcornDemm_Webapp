<table class="table table-bordered table-hover">
    <tr>
        <th>체크박스</th>
        <th>아이디</th>
        <th>닉네임</th>
        <th>코드 발급여부</th>
        <th>코드 승인상태</th>
        <th>코드</th>
        <th>코드 발급일</th>
        <th>코드 승인일</th>
    </tr>            
    <tr>           
        <% tableData.forEach(function(user) { %>    
        <td><label><input type="checkbox" value="<%=user.userid%>"/></label></td>
        <td><%=user.userid%></td>
        <td><%=user.displayname%></td>
        <td>미발급</td>
            <td>인증</td>
            <td>코드값들<br>
                <button type="button" id ="CodeBt" class="btn btn-primary btn-xs" data-toggle="modal" data-target="#myModal" data-whatever="<%=user.userid%>">code 개별발급</button>
            </td>
            <td>
                <%=user.getDate.year%> - 
                <%=user.getDate.month%> -  
                <%=user.getDate.day%>
            </td>
            <td>
                <%=user.getDate.year%> - 
                <%=user.getDate.month%> - 
                <%=user.getDate.day%>
            </td>
    </tr>            
    <%})%>
</table>

